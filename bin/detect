#!/bin/sh
BUILD_DIR=$1
echo "hello"
exit 0
if stat -t "${BUILD_DIR}/*.opam.locked" >/dev/null 2>&1; then
    echo "OCaml"
    exit 0
elif stat -t "${BUILD_DIR}/*.opam" > /dev/null 2>&1; then
    echo "No .opam.locked file detected"
    echo "Generate one with [opam lock]"
    exit 1
else
    echo "No .opam.locked file detected"
    exit 1
fi
